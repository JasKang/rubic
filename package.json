{
  "name": "rubic",
  "version": "0.3.0",
  "description": "rubic",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "miniprogram": "dist/miniprogram",
  "files": [
    "dist"
  ],
  "author": "jaskang",
  "license": "MIT",
  "homepage": "https://github.com/JasKang/rubic#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JasKang/rubic.git"
  },
  "bugs": {
    "url": "https://github.com/JasKang/rubic/issues"
  },
  "scripts": {
    "dev": "pnpm docs:dev",
    "build": "tsup && npm run build:miniprogram",
    "build:miniprogram": "node scripts/mpBuild.js",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "release": "release-it",
    "release:beta": "release-it --preRelease=beta",
    "test": "vitest run",
    "test-dts": "tsc -p ./tests-dts/tsconfig.json",
    "coverage": "vitest --coverage",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@jaskang/config": "^0.0.7",
    "@jaskang/eslint-config": "^0.0.7",
    "@release-it/conventional-changelog": "^5.1.1",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.7.14",
    "@vitest/coverage-c8": "^0.25.7",
    "@vue/reactivity": "^3.2.45",
    "c8": "^7.12.0",
    "eslint": "^8.29.0",
    "fs-extra": "^10.1.0",
    "happy-dom": "^8.1.0",
    "miniprogram-api-typings": "^3.6.0",
    "miniprogram-test-util": "^0.0.9",
    "picocolors": "^1.0.0",
    "prettier": "^2.8.1",
    "release-it": "^15.10.1",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.0",
    "vitepress": "1.0.0-alpha.31",
    "vitest": "^0.25.7",
    "vue": "^3.2.45"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "webpack",
        "@types/react",
        "react",
        "react-dom",
        "@algolia/client-search"
      ]
    }
  },
  "packageManager": "pnpm@6.24.4"
}
